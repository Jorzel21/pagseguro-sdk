{
    "name": "jorzelalves/pagseguro-sdk",
    "description": "Package for integration with PagSeguro.",
    "keywords": ["pagseguro", "laravel", "sdk"],
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*"
    },
    "license": "MIT",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Jorzel",
            "email": "jorzelalves@jorzelalves.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Jorzel\\PagSeguro\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "PagSeguro": "Jorzel\\PagSeguro\\Facades\\PagSeguro"
            }
        }
    }
}
